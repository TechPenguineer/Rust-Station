{
	"name": "ruststation",
	"displayName": "RustStation",
	"description": "An easier way to start your rust project",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ruststation.Home"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ruststation.Home",
				"title": "Go to Rust Station home page"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "9.1.1",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
